model accounts {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  access_token      String
  expires_at        Int
  id_token          String
  provider          String
  providerAccountId String
  scope             String
  token_type        String
  type              String
  userId            String @db.ObjectId
}

model chat_history {
  id        String           @id @default(auto()) @map("_id") @db.ObjectId
  chatbotId String
  chats     ChatHistoryChats
  date      String
  userId    String

  @@map("chat-history")
}

model chatbot_settings {
  id                       String   @id @default(auto()) @map("_id") @db.ObjectId
  bubbleIconUrl            String
  chatbotBubbleAlignment   String
  chatbotDisplayName       String
  chatbotIconColor         String
  chatbotId                String
  initialMessage           String[]
  instruction              String
  lastTrained              Float
  messagePlaceholder       String
  model                    String
  noOfMessagesSent         Int
  numberOfCharacterTrained String
  profilePictureUrl        String
  suggestedMessages        String[]
  temperature              Int
  theme                    String
  userId                   String
  userMessageColor         String
  visibility               String

  @@map("chatbot-settings")
}

model chatbots_data {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  chatbotId String
  content   String
  dataID    String[]
  source    String

  @@map("chatbots-data")
}

model plans {
  id                   String @id @default(auto()) @map("_id") @db.ObjectId
  messageLimit         Int
  name                 String
  numberOfChatbot      Int
  trainingDataLimit    Int
  websiteCrawlingLimit Int
}

model sessions {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  expires      DateTime @db.Date
  sessionToken String
  userId       String   @db.ObjectId
}

model user_chatbots {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  chatbotId        String
  chatbotName      String
  createdAt        Float
  lastUsed         Float
  noOfMessagesSent Int
  userId           String

  @@map("user-chatbots")
}

model user_details {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  totalMessageCount Int
  userId            String

  @@map("user-details")
}

model users {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  email         String?
  /// Could not determine type: the field only had null or empty values in the sample set.
  emailVerified Json?
  image         String?
  name          String?
  password      String?
  planId        String  @db.ObjectId
  username      String?
}

model whatsappbot_details{
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  webhookVarificationToken String?
  isVarifyWebhook Boolean
  whatsAppAccessToken String?
  facebookAppSecret String?
  whatsAppPhoneNumber String?
  phoneNumberId String?
  phoneBusinessId String?
  isActive Boolean
  userId String?
  chatbotId String?
}
